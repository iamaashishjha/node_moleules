{
	"compilerOptions": {
	  "target": "es6",
	  "module": "commonjs",
	  "esModuleInterop": true,
	  "forceConsistentCasingInFileNames": true,
	  "noImplicitAny": true,
	  "removeComments": true,
	  "preserveConstEnums": true,
	  "sourceMap": true,
	  "outDir": "./dist"
	},
	"include": ["./**/*"],
	"exclude": ["node_modules/**/*", "test"]
  }
  